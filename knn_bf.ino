      distance = sqrt(distance);

      // Serial.println("距離の計算をする");
      // Serial.println(distance); //これが同じ値になり続けているのがおかしいと思う
      // delay(500);

      // 距離が小さいなら結果を保存する
      for (int d = 0; d < kLen; d++){
        if (min_distance[d] > distance){
          float tmp = min_distance[d];
          float tmp_category = category_value[d];

          min_distance[d] = distance;
          category_value[d] = points[i][kLen * windowSize];
          for (int j = d; j < kLen - 1; j++){
            //わけわからん、もう
            float nextTmp = min_distance[j + 1];
            float nextTmpCategory = category_value[j + 1];
            min_distance[j + 1] = tmp;
            category_value[j + 1] = tmp_category;
            
            tmp = nextTmp;
            tmp_category = nextTmpCategory;
          }
          break;
        }

      // Serial.println("---jfjfjf----");
      // for (int d = 0; d < kLen; d++){ 
      //   Serial.println(min_distance[d]);
      // }
      // Serial.println("---jfjfjf----");

      }
      distance = 0.0; //distanceのリセットを忘れていた（#^ω^）ﾋﾟｷﾋﾟｷ
    }

    Serial.println("結果出たよ！");
    Serial.println("----；；---");
    for (int d = 0; d < kLen; d++){
      Serial.print(min_distance[d]);
    }
    Serial.println("");
    for (int d = 0; d < kLen; d++){
      Serial.print(category_value[d]);
    }
    Serial.println("");
    Serial.println("----；；---");
    // 対応表{'jump': 0.0, 'run': 1.0, 'stand': 2.0, 'walk': 3.0}

    // 各数字の出現回数をカウントする変数を初期化
    int count[4] = {0}; // 0.0, 1.0, 2.0, 3.0 の順でカウントする

    // 配列を走査して数字の出現回数をカウント
    for (int i = 0; i < kLen; i++) {
        // 数字が0.0, 1.0, 2.0, 3.0のいずれかであれば、該当するカウンタをインクリメント
        if (category_value[i] == 0.0) {
            count[0]++;
        } else if (category_value[i] == 1.0) {
            count[1]++;
        } else if (category_value[i] == 2.0) {
            count[2]++;
        } else if (category_value[i] == 3.0) {
            count[3]++;
        }
    }

    // 最も多く現れた数字を探す
    int maxCountIndex = 0;
    for (int i = 1; i < 4; i++) {
        if (count[i] > count[maxCountIndex]) {
            maxCountIndex = i;
        }
    }
    
    client.print(maxCountIndex);

    if (lastIndex != maxCountIndex){
      lastIndex = maxCountIndex;
      bleKeyboard.releaseAll();
      // キー入力を行う
      if (lastIndex == 0){
        bleKeyboard.write(' '); //ジャンプ
      } else if(lastIndex == 1){
        bleKeyboard.press('w'); //走るだけど仮で歩くにしておく
      } else if(lastIndex == 2){
        bleKeyboard.releaseAll(); //何もしない。棒立ち
      } else if(lastIndex == 3){
        bleKeyboard.press('w'); //歩く
      }
      
    }
    
  }

  

